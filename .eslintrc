{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
	  "jest": true
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
	"sourceType": "module"
  },
  "rules": {
    "no-console": "error",
    "react/react-in-jsx-scope": 0
  },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
}